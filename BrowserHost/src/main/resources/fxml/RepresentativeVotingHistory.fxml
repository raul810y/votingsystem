<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <HBox prefWidth="415.0">
         <children>
            <DatePicker fx:id="dateFromPicker" />
            <DatePicker fx:id="dateToPicker">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </DatePicker>
         </children>
      </HBox>
      <TextField fx:id="emailText">
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin>
      </TextField>
      <HBox prefWidth="200.0">
         <children>
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="acceptButton" mnemonicParsing="false" text="Button" />
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
